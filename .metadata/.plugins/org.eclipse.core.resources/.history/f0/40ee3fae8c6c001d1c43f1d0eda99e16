package com.wmart.operation;

import java.util.Scanner;

import com.wmart.login.User;

public class Operations {

	public static void main(String[] args) throws Exception {
		Scanner scan = new Scanner(System.in);
		System.out.println("Welcome to WMart: ");
		User user = new User();
		String y=" ",n=" ";
		String i;
		i=y;
		i=n;
		System.out.println("are you new register"+i);
		if(y=="Yes")
		{
			System.out.println("new Register"+y);
			user.Register();
		
		}
		else if(n=="No")
		{
			System.out.println("Please login here"+n);
			user.Login();
		}
		
	

	}

}
